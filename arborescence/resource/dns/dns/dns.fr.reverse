$TTL 604800
@ IN SOA site22.fr. root.site22.fr. (
        4        ; Numéro de série
        1h       ; Rafraîchissement
        10m      ; Essai
        1d       ; Expiration
        1h )     ; TTL du cache négatif

@ IN NS site22.fr.
dns IN A {IP_LAN} ; Adresse IP du serveur DNS

153 PTR site22.fr. ;
